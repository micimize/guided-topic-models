{
  "presets": [
    [ "env", { "modules": false} ],
    "react"
  ],
  "plugins": [
    [
      "react-css-modules",
      {
        "generateScopedName": "[name]__[local]___[hash:base64:5]" ,
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss"
          }
        }
      }
    ]
  ]
}